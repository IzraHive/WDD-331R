{
  "name": "style-stage-project",
  "version": "1.0.0",
  "description": "CSS Style Stage themed project",
  "main": "index.js",
  "scripts": {
    "scss": "sass --no-source-map sass:public",
    "watch": "sass --no-source-map --watch sass:public",
    "serve": "browser-sync start --server public --files public --port 1337",
    "start": "npm-run-all scss --parallel watch serve"
  },
  "keywords": [
    "css",
    "scss",
    "style-stage"
  ],
  "author": "Your Team Name",
  "license": "MIT",
  "dependencies": {
    "browser-sync": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.10"
  }
}
